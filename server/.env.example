PORT=5000
MONGODB_URI=mongodb://localhost:27017/globalconnect
JWT_SECRET=your_jwt_secret_key
JWT_EXPIRE=7d
NODE_ENV=development

# Google OAuth2 (replace these with your real credentials)
GOOGLE_CLIENT_ID=your_google_client_id
GOOGLE_CLIENT_SECRET=your_google_client_secret
# Example callback URL for local dev (must match Google Console setting):
GOOGLE_CALLBACK_URL=https://netglobalconnect.onrender.com/api/auth/google/callback

# Frontend origin used by CORS
FRONTEND_URL=https://net-global-connect.netlify.app
